# LLM provider (set at least one)
OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# GOOGLE_API_KEY=
# OPENROUTER_API_KEY=
# HUGGINGFACE_API_KEY=

# Main model (required)
MODEL_NAME=gpt-4o-mini
# Separate model for strict structured outputs (optional)
MODEL_NAME_FOR_STRUCTURED_OUTPUT=gpt-4o-mini

# Maximum number of test scenarios to generate per run (optional, default: 3)
MAX_NUMBER_OF_SCENARIOS=3

# Delay between scenario executions in milliseconds to avoid rate limiting (optional, default: 45000)
DELAY_BETWEEN_SCENARIOS=45000

# GitHub token with permissions to read PR and write comments (issues:write, pull_requests:write)
GITHUB_ACCESS_TOKEN=

# Link to PR where the report will be published. Required when using start:runa command
# PULL_REQUEST_LINK=https://github.com/owner/repo/pull/123

# GitHub App (optional; typically for CI)
# If using GitHub App token in CI, can be left empty in local environment
# APP_ID=
# APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
# ...your private key lines...
# -----END RSA PRIVATE KEY-----"
